{
  "name": "FIAB",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "uv run --frozen uvicorn forecastbox.entrypoint:app --reload --log-level info",
    "cascade-dev": "uv run --frozen --module cascade.gateway tcp://localhost:8079",
    "python-dev": "concurrently \"npm run fastapi-dev\" \"npm run cascade-dev\"",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run python-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mantine/core": "^7.17.4",
    "@mantine/dates": "^7.17.4",
    "@mantine/hooks": "^7.17.4",
    "@mantine/notifications": "^7.17.4",
    "@tabler/icons-react": "^3.30.0",
    "@types/node": "22.5.5",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "axios": "^1.8.4",
    "concurrently": "^9.0.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "next": "^14.2.13",
    "postcss": "^8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-globe": "^5.0.2",
    "react-globe.gl": "^2.33.2",
    "tailwindcss": "3.4.12",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2"
  }
}
